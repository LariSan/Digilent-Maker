<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE doc SYSTEM "../../ipdialog.dtd" [
	<!ENTITY C_USE_HDMI_RECEIVER '
	<widget id="C_USE_HDMI_RECEIVER">
		<key>C_USE_HDMI_RECEIVER</key>
		<label>Use HDMI Receiver Module </label>
		<tip></tip>
	</widget>
	'>
    <!ENTITY C_USE_HDMI_TRANSMITTER '
	<widget id="C_USE_HDMI_TRANSMITTER">
		<key>C_USE_HDMI_TRANSMITTER</key>
		<label>Use HDMI Transmitter Module </label>
		<tip></tip>
	</widget>
	'>
    <!ENTITY C_USE_HDMI_REGS '
	<widget id="C_USE_HDMI_REGS">
		<key>C_USE_HDMI_REGS</key>
		<label>Use HDMI Software Registers Module </label>
		<tip></tip>
	</widget>
	'>
    <!ENTITY C_USE_HDMI_DDC '
	<widget id="C_USE_HDMI_DDC">
